{
  "name": "vocabulaire",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build4git": "ng build --configuration production --base-href https://plrigaux.github.io/vocabulaire/",
    "deploy4git": "npx angular-cli-ghpages -d dist/vocabulaire",
    "fulldeploy": "npm run build4git && npm run deploy4git",
    "altdeploy": "ng deploy --base-href=https://plrigaux.github.io/vocabulaire/ --name='Vocabulaire' --email=plr@noreply.com"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.3",
    "@angular/cdk": "^18.2.3",
    "@angular/common": "^18.2.3",
    "@angular/compiler": "^18.2.3",
    "@angular/core": "^18.2.3",
    "@angular/forms": "^18.2.3",
    "@angular/material": "^18.2.3",
    "@angular/platform-browser": "^18.2.3",
    "@angular/platform-browser-dynamic": "^18.2.3",
    "@angular/router": "^18.2.3",
    "difflib-ts": "^1.0.3",
    "json5": "^2.2.1",
    "marked": "^4.2.1",
    "process": "^0.11.10",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.4",
    "@angular/cli": "^18.2.4",
    "@angular/compiler-cli": "^18.2.3",
    "@types/jasmine": "~3.10.0",
    "@types/marked": "^4.0.7",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~5.5.4"
  }
}